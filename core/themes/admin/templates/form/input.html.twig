{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see\Drupal\Core\Form\FormPreprocess::preprocessInput()
 * @see gin_preprocess_input()
 */
#}
{% if autocomplete_message %}
  <div class="gin-autocomplete">
    <input{{ attributes }}/><div class="gin-autocomplete__message hidden" data-drupal-selector="autocomplete-message">{{ autocomplete_message }}</div>
  </div>
  {{- children -}}
{% else %}
  <input{{ attributes }}/>{{- children -}}
{% endif %}
